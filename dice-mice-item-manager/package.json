{
  "name": "dice-mice-item-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "build:production": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run lint && npm run test:coverage -- --watchAll=false && npm run build",
    "db:generate": "drizzle-kit generate",
    "db:generate:production": "cross-env NODE_ENV=production drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:production": "cross-env NODE_ENV=production drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:studio:production": "cross-env NODE_ENV=production drizzle-kit studio",
    "deploy:production": "bash deploy-production.sh"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@headlessui/react": "^2.2.9",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.11.5",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20"
  }
}
